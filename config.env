# OpenAI API Konfiguration
OPENAI_API_KEY=your-openai-api-key-here

# System Konfiguration
MODEL_NAME=gpt-3.5-turbo
TEMPERATURE=0.7

# Server Konfiguration
HOST=127.0.0.1
PORT=8001

# Marken-Konfiguration
DEFAULT_BRAND=heine
